{
  "name": "express-react-app",
  "version": "1.0.0",
  "description": "React app starter, powered by Express as backend server.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public",
    "dev": "run-p dev:client dev:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "dev:server": "cross-env NODE_ENV=development nodemon --watch server index.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --hide-modules",
    "lint": "npm-run-all lint:js lint:style",
    "lint:js": "eslint . --fix",
    "lint:style": "stylelint **/*.{css,scss,sass} --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "keywords": [
    "react",
    "express",
    "starter"
  ],
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "helmet": "^4.0.0",
    "hpp": "^0.2.3",
    "make-promises-safe": "^5.1.0",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^4.2.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.3",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^4.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpackbar": "^4.0.0"
  }
}
