{
  "name": "express-react-app",
  "version": "3.3.0",
  "description": "React app starter, powered by Express as backend server.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public",
    "dev": "run-p dev:client dev:server",
    "dev:client": "cross-env NODE_ENV=development webpack serve",
    "dev:server": "cross-env NODE_ENV=development nodemon --watch server .",
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "build:analyze": "cross-env ANALYZE_MODE=true npm run build",
    "start": "cross-env NODE_ENV=production node .",
    "lint": "npm-run-all lint:js lint:style",
    "lint:js": "eslint . --fix",
    "lint:style": "stylelint **/*.{css,scss,sass} --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:update": "npm test -- --updateSnapshot",
    "test:coverage": "rimraf storage/coverage && npm test -- --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/express-react-app.git"
  },
  "engines": {
    "npm": ">=6.12.0",
    "node": ">=12.13.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE 10"
  ],
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "keywords": [
    "react",
    "express",
    "starter"
  ],
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "make-promises-safe": "^5.1.0",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression-webpack-plugin": "^8.0.1",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^2.0.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "nodemon": "^2.0.9",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.41.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
